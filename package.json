{
  "name": "@nuxtjs/router",
  "version": "1.1.0",
  "description": "Nuxt module to use router.js instead of pages/ directory",
  "license": "MIT",
  "contributors": [
    {
      "name": "Sebastien Chopin <seb@chopin.io>"
    }
  ],
  "main": "index.js",
  "repository": "https://github.com/nuxt-community/router-module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint index.js",
    "lint-fix": "eslint --fix lib src",
    "test": "NODE_ENV=test yarn run lint && jest",
    "release": "standard-version && git push --follow-tags && yarn publish"
  },
  "files": [
    "index.js"
  ],
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src"
    ]
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "jest-cli": "^23.4.2",
    "nuxt": "^2.3.1"
  }
}
